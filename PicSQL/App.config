    <add key="dbDataSource_SQLite" value="" />
    <add key="dbInitialCatalog_SQLite" value="" />
    <add key="dbPassword_SQLite" value="" />
    <add key="dbImageTable_SQLite" value="IMAGE" />
    <add key="dbServer_MySQL" value="" />
